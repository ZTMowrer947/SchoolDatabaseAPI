{
    "restartable": "rs",
    "ignore": [".git", "./node_modules/**"],
    "watch": ["src"],
    "verbose": true,
    "env": {
        "NODE_ENV": "development",
        "DB_ENABLE_LOGGING": "false",
        "ENABLE_GLOBAL_ERROR_LOGGING": "false"
    },
    "exec": "babel-node --extensions '.ts' src",
    "ext": "ts"
}
